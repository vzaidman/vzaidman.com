{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "routes": [
    {
      "src": "/(.+)",
      "status": 301,
      "headers": {
        "Location": "https://vzaidman.com"
      }
    },
    {
      "src": ".*",
      "headers": {
        "X-Frame-Options": "ALLOWALL"
      }
    }
  ]
}
