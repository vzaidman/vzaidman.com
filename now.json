{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "routes": [
    {
      "src": "/^((?!(pdf|png|txt|xml)).)+$",
      "status": 301,
      "headers": {
        "Location": "https://vzaidman.com"
      }
    },
    {
      "src": "/.*\\.png$",
      "headers": {
        "Cache-Control": "max-age=3600000"
      }
    },
    {
      "src": ".*",
      "headers": {
        "X-Frame-Options": "DENY",
        "Strict-Transport-Security": "max-age=63072000; includeSubDomains; preload",
        "X-Content-Type-Options": "nosniff",
        "X-XSS-Protection": "1; mode=block",
        "Link": [
          "<https://www.googletagmanager.com/gtag/js?id=UA-142161010-1>; rel=prefetch; crossorigin",
          "<https://www.google-analytics.com/analytics.js>; rel=prefetch; crossorigin"
        ]
      }
    }
  ]
}
